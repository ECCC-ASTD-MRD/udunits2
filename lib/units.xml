<?xml version="1.0"? encoding="ISO-8859-1"?>

<unitsystem>
    <!-- Prefixes -->
    <prefix name="yotta" symbol="Y" value="1e24"/>
    <prefix name="zetta" symbol="Z" value="1e21"/>
    <prefix name="exa" symbol="E" value="1e18"/>
    <prefix name="peta" symbol="P" value="1e15"/>
    <prefix name="tera" symbol="T" value="1e12"/>
    <prefix name="giga" symbol="G" value="1e9"/>
    <prefix name="mega" symbol="M" value="1e6"/>
    <prefix name="kilo" symbol="k" value="1e3"/>
    <prefix name="hecto" symbol="h" value="100"/>
    <prefix name="deka" symbol="da" value="10"/>
    <prefix name="deci" symbol="d" value=".1"/>
    <prefix name="centi" symbol="c" value=".01"/>
    <prefix name="milli" symbol="m" value="1e-3"/>
    <prefix name="micro" symbol="u" latin1Symbol="µ" value="1e-6"/>
    <prefix name="nano" symbol="n" value="1e-9"/>
    <prefix name="pico" symbol="p" value="1e-12"/>
    <prefix name="femto" symbol="f" value="1e-15"/>
    <prefix name="atto" symbol="a" value="1e-18"/>
    <prefix name="zepto" symbol="z" value="1e-21"/>
    <prefix name="yocto" symbol="y" value="1e-24"/>

    <!-- Base Units -->
    <baseunit name="meter" symbol="m"/>
    <baseunit name="kilogram" symbol="kg"/>
    <baseunit name="second" symbol="s"/>
    <baseunit name="ampere" symbol="A"/>
    <baseunit name="kelvin" symbol="K"/>
    <baseunit name="mole" symbol="mol"/>
    <baseunit name="candela" symbol="cd"/>

    <!-- Dimensionless Units -->
    <dimensionlessunit name="radian" symbol="rad"/>

    <!-- Solid Angle -->
	<unit name="steradian" symbol="sr">rad²</unit>

    <!-- Frequency -->
	<unit name="hertz" symbol="Hz">1/s</unit>

    <!-- Force -->
	<unit name="newton" symbol="N">m·kg/s²</unit>

    <!-- Pressure, Stress -->
	<unit name="pascal" symbol="Pa">N/m²</unit>

    <!-- Energy, Work, Quantity of Heat -->
	<unit name="joule" symbol="J">N·m</unit>

    <!-- Power, Radiant Flux -->
	<unit name="watt" symbol="W">J/s</unit>

    <!-- Electricity -->
	<unit name="coulomb" symbol="C">s·A</unit>
	<unit name="volt" symbol="V">W/A</unit>
	<unit name="farad" symbol="F">C/V</unit>
	<unit name="ohm" utf8Symbol="Î©">V/A</unit>
	<unit name="siemens" plural="siemens" symbol="S">A/V</unit>

	<alias name="abampere">10 A</alias>
	<alias name="gilbert">7.957747e-1 A</alias>
	<alias name="statampere">3.335640e-10 A</alias>
	<alias name="biot">10 A</alias>

    <!-- Magnetic Flux -->
	<unit name="weber" symbol="Wb">V·s</unit>

    <!-- Magnetic Flux Density -->
	<unit name="tesla" symbol="T">Wb/m²</unit>

    <!-- Inductance -->
	<unit name="henry" symbol="H">Wb/A</unit>

    <!-- Thermodynamic Temperature -->
	<alias name="degree_kelvin" plural="degrees_kelvin" 
	    latin1Symbol="°K">K</alias>
	<alias name="deg_K" plural="degs_K">K</alias>
	<alias name="degree_K" plural="degrees_K">K</alias>

	<alias name="degree_celsius" plural="degrees_celsius" 
	    latin1Symbol="°C">K @ 273.15</alias>
	<alias name="celsius" plural="celsius">°C</alias>
	<alias name="deg_C" plural="degs_C">°C</alias>
	<alias name="degree_C" plural="degrees_C">°C</alias>

	<alias name="degree_rankine" plural="degrees_rankine" 
	    latin1Symbol="°R">K/1.8</alias>
	<alias name="rankine">°R</alias>
	<alias name="deg_R" plural="degs_R">°R</alias>
	<alias name="degree_R" plural="degrees_R">°R</alias>

	<alias name="degree_fahrenheit" plural="degrees_fahrenheit" symbol="F"
	    latin1Symbol="°F">°R @ 459.67</alias>
	<alias name="fahrenheit">°F/alias>
	<alias name="deg_F" plural="degs_F">°F</alias>
	<alias name="degree_F" plural="degrees_F">°F</alias>

    <!-- Mass -->
	<alias name="assay_ton">2.916667e-2 kg</alias>
	<alias name="avoirdupois_ounce">2.834952e-2 kg</alias>
	<alias name="avoirdupois_pound">4.5359237e-1 kg</alias>
	<alias name="carat">2e-4 kg</alias>
	<alias name="grain" symbol="gr">6.479891e-5 kg</alias>
	<alias name="long_hundredweight">5.080235e1 kg</alias>
	<alias name="metric_ton" symbol="t">1e3 kg</alias>
	<alias name="pennyweight">1.555174e-3 kg</alias>
	<alias name="short_hundredweight">4.535924e1 kg</alias>
	<alias name="slug">14.59390 kg</alias>
	<alias name="troy_ounce">3.110348e-2 kg</alias>
	<alias name="troy_pound">3.732417e-1 kg</alias>
	<alias name="atomic_mass_unit" symbol="u">1.66054e-27 kg</alias>

	<alias name="tonne">metric_ton</alias>
	<alias name="apothecary_ounce">troy_ounce</alias>
	<alias name="apothecary_pound">troy_pound</alias>
	<alias name="pound" symbol="lb">avoirdupois_pound</alias>
	<alias name="scruple">20 grain</alias>
	<alias name="apdram">60 grain</alias>
	<alias name="apounce">480 grain</alias>
	<alias name="appound">5760 grain</alias>

	<alias name="bag">94 pound</alias>
	<alias name="short_ton">2000 pound</alias>
	<alias name="long_ton">2240 pound</alias>

	<alias name="ton">short_ton</alias>

    <!-- Length -->
	<alias name="angstrom" symbol="Å">1e-10 m</alias>
	<alias name="astronomical_unit" symbol="au">1.495979e11 m</alias>
	<alias name="fermi">1e-15 m</alias>
	<alias name="metre">m</alias>
	<alias name="light_year">9.46073e15 m</alias>
	<alias name="micron">1e-6 m</alias>
	<alias name="mil">2.54e-5 m</alias>
	<alias name="nautical_mile" symbol="nmi">1.852000e3 m</alias>
	<alias name="parsec">3.085678e16 m</alias>
	<alias name="printers_point">3.514598e-4 m</alias>
	<alias name="chain">2.011684e1 m</alias>

	<alias name="printers_pica">12 printers_point</alias>
	<alias name="nmile">nautical_mile</alias>

	<alias name="US_survey_foot"
	    plural="US_survey_feet">(1200/3937) m></alias>
	<alias name="US_survey_yard">3 US_survey_feet</alias>
	<alias name="US_survey_mile">5280 US_survey_feet</alias>
	<alias name="US_statute_mile">US_survey_mile</alias>
	<alias name="rod">16.5 US_survey_feet</alias>
	<alias name="pole">rod</alias>
	<alias name="perch">rod</alias>
	<alias name="furlong">660 US_survey_feet</alias>
	<alias name="fathom">6 US_survey_feet</alias>

	<alias name="international_inch">2.54 cm</alias>
	<alias name="international_foot"
	    plural="international_feet">12 international_inches</alias>
	<alias name="international_yard">3 international_feet</alias>
	<alias name="international_mile">5280 international_feet</alias>

	<alias name="inch" symbol="in">international_inch</alias>
	<alias name="foot" plural="feet" symbol="ft">international_foot</alias>
	<alias name="yard" symbol="yd">international_yard</alias>
	<alias name="mile" symbol="mi">international_mile</alias>

	<alias name="pica">printers_pica</alias>
	<alias name="big_point">inch/72</alias>
	<alias name="barleycorn">inch/3</alias>
	<alias name="arpentlin">191.835 foot</alias>

    <!-- Time -->
	<alias name="day" symbol="d">8.64e4 s</alias>
	<alias name="hour" symbol="h">3.6e3 s</alias>
	<alias name="minute" symbol="min">60 s</alias>
	<alias name="sec">s</alias>
	<alias name="shake">1e-8 s</alias>
	<alias name="sidereal_day">8.616409e4 s</alias>
	<alias name="sidereal_hour">3.590170e3 s</alias>
	<alias name="sidereal_minute">5.983617e1 s</alias>
	<alias name="sidereal_second">0.9972696 s</alias>
	<alias name="sidereal_year">3.155815e7 s</alias>
	<!--
            Interval between 2 successive passages of sun
            through vernal equinox (365.242198781 days -- see
            <http://www.ast.cam.ac.uk/pubinfo/leaflets/>,
            <http://aa.usno.navy.mil/AA/>, and
            <http://adswww.colorado.edu/adswww/astro_coord.html>):
	-->
	<alias name="tropical_year">3.15569259747e7 s</alias>
	<alias name="lunar_month">29.530589 day</alias>

	<alias name="common_year">365 day</alias>
	<alias name="leap_year">366 day</alias>
	<alias name="Julian_year">365.25 day</alias>
	<alias name="Gregorian_year">365.2425 day</alias>
	<alias name="sidereal_month">27.321661 day</alias>
	<alias name="tropical_month">27.321582 day</alias>
	<alias name="fortnight">14 day</alias>
	<alias name="week">7 day</alias>
	<alias name="jiffy">0.01 s</alias>
	<alias symbol="hr">hour</alias>

	<alias name="year" symbol="a">tropical_year</alias>

	<alias symbol="yr">year</alias>
	<alias name="eon">1e9 year</alias>
	<alias name="month">year/12</alias>

    <!-- Illumination -->
	<unit name="lumen" symbol="lm">cd·sr</unit>
	<unit name="lux" symbol="lx">lm/m²</unit>
	<alias name="candle">cd</alias>

    <!-- Activity (of a Radionuclide) -->
	<unit name="becquerel" symbol="Bq">1/s</unit>

    <!-- Absorbed Dose, Specific Energy (Imparted), Kerma -->
	<unit name="gray" symbol="Gy">J/kg</unit>

    <!-- Dose equivalent -->
	<unit name="sievert" symbol="Sv">J/kg</unit>

    <!-- Catalytic Activity -->
	<unit name="katal" symbol="kat">mol/s</unit>




    <!-- Units with Special Names or Symbols (definition will map to name 
    or symbol)-->
	<unit name="steradian" symbol="sr">rad²</unit>
	<unit name="gram" symbol="g">1e-3 kg</unit>
	<unit symbol="BZ">lg(re um³)</unit>

    <!-- Unit Aliases (definition won't map to name or symbol) -->
	<!-- Thermodynamic Temperature -->
</unitsystem>
